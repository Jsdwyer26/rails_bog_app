<h1>New Creature</h1>
<hr>
<%= form_for @creature do |f| %>
	<div class ="form-group">
  		<%= f.text_field :name , class: "form-control"%>
  	</div>
  	<div class ="form-group">
  		<%= f.text_area :description, class: "form-control" %>
  	</div>
  <%= f.submit "Save Creature", class: "btn btn-block btn-success" %>
<% end %>
